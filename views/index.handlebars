<div class="article-section">
  {{#each articles}}
  <div class="row">
    <div class="col-lg-6" id="article-content">
      <h2> {{this.title}} </h2>
      <br>
      <p> {{this.body}}</p>
      <br>
      <p> {{this.link}}</p>
    </div>

    <button type="button" class="" data-id={{this._id}} id="save-article" saved={{this.saved}}>Save Article</button>
    <button type="button" class="" id='article-comment' data-id={{this._id}}> View Article Note </button>

    <div class="col-lg-6" id="comments">

    </div>
  </div>
  {{/each}}
</div>